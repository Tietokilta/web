{
  "name": "@tietokilta/ui",
  "version": "0.0.0-alpha.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tietokilta/web.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./global.css": {
      "types": "./dist/global.d.ts",
      "default": "./dist/global.css"
    },
    "./preset": {
      "import": {
        "types": "./dist/preset.d.mts",
        "default": "./dist/preset.mjs"
      },
      "require": {
        "types": "./dist/preset.d.ts",
        "default": "./dist/preset.js"
      }
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup lib/index.tsx lib/global.css lib/preset.ts --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup lib/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint lib/",
    "lint:fix": "eslint --fix lib/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tietokilta/config-typescript": "workspace:*",
    "@tietokilta/eslint-config": "workspace:*",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "peerDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
