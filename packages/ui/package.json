{
  "name": "@tietokilta/ui",
  "version": "0.0.0-alpha.5",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup lib/index.tsx lib/global.css --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup lib/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint lib/",
    "lint:fix": "eslint --fix lib/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "catalog:",
    "lucide-react": "^0.544.0",
    "tailwind-merge": "catalog:"
  },
  "devDependencies": {
    "@tietokilta/config-typescript": "workspace:*",
    "@tietokilta/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "peerDependencies": {
    "@tailwindcss/postcss": "catalog:",
    "@tailwindcss/typography": "catalog:",
    "postcss": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
